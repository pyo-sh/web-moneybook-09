{
    "compilerOptions": {
        "checkJs": false,
        "baseUrl": "./src",
        "paths": {
            "@src/*": ["./*"],
            "@apis/*": ["./apis/*"],
            "@components/*": ["./components/*"],
            "@core/*": ["./core/*"],
            "@pages/*": ["./pages/*"],
            "@store/*": ["./store/*"],
            "@utils/*": ["./utils/*"]
        },
        "moduleResolution": "node"
    },
    "exclude": ["node_modules"]
}
